{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "ios-simulator": {
      "extends": "development",
      "channel": "ios-simulator",
      "ios": {
        "simulator": true
      }
    },
    "internal": {
      "autoIncrement": true,
      "distribution": "internal",
      "channel": "staging"
    },
    "staging": {
      "autoIncrement": true,
      "channel": "staging"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "staging": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    },
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "completed"
      }
    }
  }
}
